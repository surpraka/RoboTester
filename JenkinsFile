pipeline {
  agent any
  
  stages {
	
	stage('Checkout'){
	  steps{
		git branch: 'develop' , credentialsId: 'BitBucket RoboTester' , url : 'https://del.tools.publicis.sapient.com/bitbucket/scm/dam/robotester.git'
	  }
	}
	
	stage('Start Java Gateway'){
	  steps{
		start cmd /k java -jar ExtentReportjar.jar
	  }
	}
    
	stage('Install Required Lib') {
      steps {
		call imports.bat
      }
    }
	
	stage('Start Regression'){
	  steps{
		call run_python.bat
		pause
	  }
	}
  }
}